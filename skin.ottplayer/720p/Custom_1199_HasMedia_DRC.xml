<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1199">
	<!--        Player.Playing        !Player.Caching          VideoPlayer.IsFullscreen     !Player.Seeking         -->
	<visible>Skin.HasSetting(ExperimentalVolumeAmplificationUseAutomaticPresetSwitching) +      Player.Playing +    Player.HasMedia + !String.IsEqual(Player.Filenameandpath,Window(home).Property(DRCid)) + [[Player.HasVideo + VideoPlayer.IsFullscreen] | [!Player.HasVideo + Player.HasAudio]]</visible>
	<!--
	<visible>Skin.HasSetting(ExperimentalVolumeAmplificationUseAutomaticPresetSwitching) +      Player.Playing +    Player.HasMedia + !String.IsEqual(Player.Filenameandpath,Window(home).Property(DRCid)) + [[Player.HasVideo + VideoPlayer.IsFullscreen] | [!Player.HasVideo + Player.HasAudio + !Pvr.IsPlayingRadio]] | [!Player.HasVideo + Player.HasAudio + Pvr.IsPlayingRadio + VideoPlayer.IsFullscreen]]</visible>
	-->
	<!--<onload>Notification(HasMedia,$INFO[Window(home).Property(DRCid)])</onload>-->
	
	<include>VolumeAmplification_Volume_Preset_Vars_OnLoad</include>
	
	<include condition="!Player.Passthrough + !String.IsEqual(Player.Filenameandpath,Window(home).Property(DRCid)) + [[!String.StartsWith(Window(home).Property(DRCidFileName),pvr.) + !String.EndsWith(Window(home).Property(DRCidFileName),.pvr)] | [!String.StartsWith(Player.Filename,pvr.) + !String.EndsWith(Player.Filename,.pvr)]] +        ![!Player.IsInternetStream + VideoPlayer.Content(Movies)]">VolumeAmplification_Preset_DRC_OnLoad_OFF</include>
	<include condition="!Player.Passthrough + !String.IsEqual(Player.Filenameandpath,Window(home).Property(DRCid)) + [[!String.StartsWith(Window(home).Property(DRCidFileName),pvr.) + !String.EndsWith(Window(home).Property(DRCidFileName),.pvr)] | [!String.StartsWith(Player.Filename,pvr.) + !String.EndsWith(Player.Filename,.pvr)]] +         [!Player.IsInternetStream + VideoPlayer.Content(Movies)]">VolumeAmplification_Preset_DRC_OnLoad_ON</include>
	
	<!--
	<onload condition="!String.IsEqual(Player.Filenameandpath,Window(home).Property(DRCid)) + VideoPlayer.Content(LiveTV) + [String.StartsWith(Window(home).Property(DRCid),pvr.) | String.EndsWith(Window(home).Property(DRCid),.pvr)]">SetVolume(100,false)</onload>
	-->
	
	<onload condition="!Player.Passthrough">SetProperty(DRCid,"$INFO[Player.Filenameandpath]",home)</onload>
	<onload condition="!Player.Passthrough">SetProperty(DRCidFileName,"$INFO[Player.Filename]",home)</onload>
	
	<onload condition="Player.Passthrough">Notification(Can't use VolumeAmplification-DRC,DISABLE audio passthrough first,5000,DefaultIconError.png)</onload>
	
	<!--<onload>Notification(HasMedia,$INFO[Window(home).Property(DRCid)],7500)</onload>-->
	
	<controls>
	</controls>
</window>
